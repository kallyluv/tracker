{
	"name": "tracker",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
		"dev:client": "vite",
		"dev:server": "node --watch dist/load.js",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"start": "npm run build && node dist/load.js"
	},
	"dependencies": {
		"bcryptjs": "^3.0.3",
		"cors": "^2.8.5",
		"dotenv": "^17.2.3",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.3",
		"mysql2": "^3.16.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.20.0"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.6",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/react": "^19.2.8",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^4.2.0",
		"concurrently": "^8.2.2",
		"typescript": "^5.9.3",
		"vite": "^5.0.0"
	}
}
